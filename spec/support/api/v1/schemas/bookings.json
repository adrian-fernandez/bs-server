{
  "type": "object",
  "required": ["bookings"],
  "properties": {
    "booking": {
      "type": "object",
      "required": ["id", "price", "days", "rental_id", "user_id"],
      "properties": {
        "id": { "type": "integer" },
        "name": { "type": "string" },
        "price": { "type": "float" },
        "days": { "type": "integer" },
        "rental_id": { "type": "integer" },
        "user_id": { "type": "integer" },
        "client_id": { "type": "integer" },
        "start_at": { "type": "date" },
        "end_at": { "type": "date" }
      }
    },
    "meta": {
      "type": "object",
      "required": ["total_pages"],
      "properties": {
        "total_pages": { "type": "integer" },
        "current_page": { "type": "integer" }
      }
    }
  }
}
